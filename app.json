{
  "name": "MM Trading BOT",
  "description": "A BOT that will be use to allow Trading in Binance",
  "repository": "https://heroku.com/deploy?template=https://github.com/jimerictibayan2012-netflix/MM-Strategy-2/tree/master",
  "logo": "https://node-js-sample.herokuapp.com/node.png",
  "keywords": ["python", "binance", "Trading"],
  "env": {
    "WEBHOOK_PASSPHRASE": {
      "description": "Passphrase Password for Tradingview Alerts",
      "value": "<Put the Passphrase Here>"
    },
    "API_KEY": {
      "description": "Binance API Key",
      "value": "<Put the Binance API Key Here>"
    },

    "API_SECRET": {
      "description": "Binance API Secret",
      "value": "<Put the Binance API Secret Here>"
    },

    "MAIL_USERNAME": {
      "description": "Mailtrap.IO Python Username",
      "value": "<Put the Mailtrap IO Username Here>"
    },

    "MAIL_PASSWORD": {
      "description": "Mailtrap.IO Python Password",
      "value": "<Put the Mailtrap IO Username Here>"
    },
    "RISK": {
      "description": "Risk %. Currently set at 1%",
      "value": "0.01"
    },
    "TEST": {
      "description": "Mode of the BOT: 1 = Test Mode, 0 = Live Trading, 2 = Fixed Account Balance",
      "value": "0.01"
    },
    "TEST_ACCOUNT": {
      "description": "Used for Mode 2",
      "value": "1000"
    }
  }
}
